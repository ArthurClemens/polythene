    "polythene-mithril": "1.7.5",
    "polythene-style": "1.7.5",
    "polythene-tests": "1.7.5",
    "polythene-utilities": "1.7.5"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
