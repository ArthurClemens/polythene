{
  "name": "polythene-tests-cypress-react",
  "version": "1.7.5",
  "private": true,
  "main": "dist/js/index",
  "module": "dist/js/index.mjs",
  "scripts": {
    "dev": "npm-run-all --parallel dev:watch dev:serve",
    "dev:serve": "../../node_modules/.bin/webpack serve --config ../../scripts/bundling/webpack.config.dev.js --port 3000 --host 0.0.0.0",
    "dev:watch": "../../node_modules/webpack/bin/webpack.js --watch --config ../../scripts/bundling/webpack.config.dev.js",
    "build": "../../node_modules/webpack/bin/webpack.js --config ../../scripts/bundling/webpack.config.prod.js",
    "serve": "http-server -p 5000 dist"
  },
  "dependencies": {
    "cyano-react": "^0.6.8",
    "polythene-css": "1.7.5",
    "polythene-react": "1.7.5",
    "polythene-tests": "1.7.5",
    "polythene-tests-cypress":  "^1.5.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
