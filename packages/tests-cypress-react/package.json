{
  "name": "polythene-tests-cypress-react",
  "version": "1.7.5",
  "private": true,
  "main": "dist/js/index",
  "module": "dist/js/index.mjs",
  "scripts": {
    "dev": "npm-run-all --parallel dev:watch dev:serve",
    "dev:serve": "../../node_modules/.bin/webpack serve --config ../../scripts/bundling/webpack.config.dev.js --port 3000 --host 0.0.0.0",
    "dev:watch": "../../node_modules/webpack/bin/webpack.js --watch --config ../../scripts/bundling/webpack.config.dev.js",
    "build": "../../node_modules/webpack/bin/webpack.js --config ../../scripts/bundling/webpack.config.prod.js",
    "serve": "http-server -p 8080 dist",
    "test": "npm run build && start-server-and-test serve 8080 cypress:run",
    "test:i": "npm run build && npm-run-all --parallel dev cypress:open",
    "cypress:run": "../../node_modules/cypress/bin/cypress run",
    "cypress:open": "../../node_modules/cypress/bin/cypress open --config baseUrl=http://localhost:3000/#,integrationFolder=../tests-cypress-specs,testFiles=**/*.spec.js,video=false"
  },
  "dependencies": {
    "cyano-react": "^0.6.9",
    "polythene-css": "1.7.5",
    "polythene-react": "1.7.5",
    "polythene-tests": "1.7.5",
    "polythene-tests-cypress":  "^1.5.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "http-server": "^0.12.3",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^1.12.1"
  }
}
