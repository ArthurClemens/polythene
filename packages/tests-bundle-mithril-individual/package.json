{
  "name": "polythene-tests-bundle-mithril-individual",
  "version": "1.7.1",
  "description": "",
  "private": true,
  "main": "dist/js/test.js",
  "module": "dist/js/test.mjs",
  "scripts": {
    "clean": "rm -rf dist/js/*",
    "dev": "npm-run-all --parallel dev:watch dev:serve",
    "dev:serve": "../../node_modules/webpack-dev-server/bin/webpack-dev-server.js --config ../../scripts/bundling/webpack.config.dev.js --disableHostCheck true --port 3000 --host 0.0.0.0",
    "dev:watch": "../../node_modules/webpack/bin/webpack.js --watch --config ../../scripts/bundling/webpack.config.dev.js",
    "webpack": "../../node_modules/webpack/bin/webpack.js --config ../../scripts/bundling/webpack.config.prod.js",
    "analyse": "ANALYSE=1 npm run webpack",
    "build": "npm run clean && npm run webpack",
    "serve": "http-server -c-1 -p 5000 dist"
  },
  "dependencies": {
    "mithril": ">=1.1.6",
    "polythene-css-card": "1.7.1",
    "polythene-css-typography": "1.7.1",
    "polythene-mithril-button": "1.7.1",
    "polythene-mithril-card": "1.7.1",
    "polythene-mithril-checkbox": "1.7.1",
    "polythene-mithril-dialog": "1.7.1",
    "polythene-mithril-fab": "1.7.1",
    "polythene-mithril-icon": "1.7.1",
    "polythene-mithril-icon-button": "1.7.1",
    "polythene-mithril-material-design-spinner": "1.7.1",
    "polythene-mithril-notification": "1.7.1",
    "polythene-mithril-radio-group": "1.7.1",
    "polythene-mithril-raised-button": "1.7.1",
    "polythene-mithril-slider": "1.7.1",
    "polythene-mithril-snackbar": "1.7.1",
    "polythene-mithril-svg": "1.7.1",
    "polythene-mithril-switch": "1.7.1",
    "polythene-mithril-tabs": "1.7.1",
    "polythene-mithril-textfield": "1.7.1"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
