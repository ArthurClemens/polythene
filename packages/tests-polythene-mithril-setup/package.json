{
  "name": "tests-polythene-mithril-setup",
  "private": true,
  "description": "Polythene for Mithril setup example",
  "version": "1.7.4",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist/js/*",
    "dev": "npm-run-all --parallel dev:watch dev:serve",
    "dev:serve": "../../node_modules/.bin/webpack serve --config ../../scripts/bundling/webpack.config.dev.js --port 3000 --host 0.0.0.0",
    "dev:watch": "../../node_modules/webpack/bin/webpack.js --watch --config ../../scripts/bundling/webpack.config.dev.js",
    "webpack": "../../node_modules/webpack/bin/webpack.js --config ../../scripts/bundling/webpack.config.prod.js",
    "build": "npm run clean && npm run webpack",
    "serve": "http-server -c-1 -p 5000 dist"
  },
  "dependencies": {
    "mithril": ">=1.1.6",
    "polythene-css": "1.7.4",
    "polythene-mithril": "1.7.4"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
