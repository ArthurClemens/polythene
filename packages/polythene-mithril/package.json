    "polythene-mithril-dialog-pane": "^1.6.4",
    "polythene-mithril-drawer": "^1.6.4",
    "polythene-mithril-fab": "^1.6.4",
    "polythene-mithril-icon": "^1.6.4",
    "polythene-mithril-icon-button": "^1.6.4",
    "polythene-mithril-ios-spinner": "^1.6.4",
    "polythene-mithril-list": "^1.6.4",
    "polythene-mithril-list-tile": "^1.6.4",
