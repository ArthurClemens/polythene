{
  "name": "tests-render-react-typescript",
  "version": "1.7.5",
  "description": "",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist/js/*",
    "dev": "npm-run-all --parallel dev:watch dev:serve",
    "dev:serve": "../../node_modules/.bin/webpack serve --config ../../scripts/bundling/webpack.config.dev.js --port 3000 --host 0.0.0.0",
    "dev:watch": "../../node_modules/webpack/bin/webpack.js --watch --config ../../scripts/bundling/webpack.config.dev.js",
    "webpack": "ENTRY=./src/index.tsx ../../node_modules/webpack/bin/webpack.js --config ../../scripts/bundling/webpack.config.prod.js",
    "build": "npm run clean && npm run webpack",
    "serve": "http-server -p 5000 dist",
    "typecheck": "../../node_modules/typescript/bin/tsc --noEmit --strict --esModuleInterop --jsx react --esModuleInterop --skipLibCheck src/index.tsx"
  },
  "dependencies": {
    "polythene-css": "1.7.5",
    "polythene-react": "1.7.5",
    "polythene-utilities": "1.7.5",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.8.5",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.5.3",
    "typings": "^2.1.1"
  }
}
