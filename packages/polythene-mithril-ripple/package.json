{
  "name": "polythene-mithril-ripple",
  "version": "1.6.5",
  "description": "",
  "main": "dist/polythene-mithril-ripple",
  "module": "dist/polythene-mithril-ripple.mjs",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist/*",
