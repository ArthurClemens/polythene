{
  "name": "polythene-css",
  "version": "1.5.5",
  "description": "",
  "main": "dist/polythene-css",
  "module": "dist/polythene-css.mjs",
  "sideEffects": true,
  "scripts": {
    "clean": "rm -rf dist/*",
    "rollup": "../../node_modules/rollup/bin/rollup -c ../../scripts/bundling/rollup.umd.js && ../../node_modules/rollup/bin/rollup -c ../../scripts/bundling/rollup.es.js",
    "build:standalone": "MODULE_NAME=polytheneCSS ENTRY=scripts/standalone.js DEST=./dist/polythene-css-standalone ../../node_modules/rollup/bin/rollup -c ../../scripts/bundling/rollup.browser.js",
    "copy-css": "node ./scripts/copyCSS.js",
    "combine-css": "node ./scripts/combineCSS.js",
    "build": "npm run rollup && npm run build:standalone && npm run copy-css && npm run combine-css",
    "typecheck": "../../node_modules/typescript/bin/tsc --noEmit --strict --allowJs --checkJs --noImplicitAny false --lib es2015,DOM,ScriptHost --esModuleInterop true src/index.js"
  },
  "files": [
    "dist",
    "index.d.ts",
    "README.md"
  ],
  "dependencies": {
    "polythene-core-css": "1.5.5",
    "polythene-css-base-spinner": "1.5.5",
    "polythene-css-button": "1.5.5",
    "polythene-css-button-group": "1.5.5",
    "polythene-css-card": "1.5.5",
    "polythene-css-checkbox": "1.5.5",
    "polythene-css-dialog": "1.5.5",
    "polythene-css-dialog-pane": "1.5.5",
    "polythene-css-drawer": "1.5.5",
    "polythene-css-fab": "1.5.5",
    "polythene-css-icon": "1.5.5",
    "polythene-css-icon-button": "1.5.5",
    "polythene-css-ios-spinner": "1.5.5",
    "polythene-css-list": "1.5.5",
    "polythene-css-list-tile": "1.5.5",
    "polythene-css-material-design-progress-spinner": "1.5.5",
    "polythene-css-material-design-spinner": "1.5.5",
    "polythene-css-menu": "1.5.5",
    "polythene-css-notification": "1.5.5",
    "polythene-css-radio-button": "1.5.5",
    "polythene-css-ripple": "1.5.5",
    "polythene-css-search": "1.5.5",
    "polythene-css-selection-control": "1.5.5",
    "polythene-css-shadow": "1.5.5",
    "polythene-css-slider": "1.5.5",
    "polythene-css-snackbar": "1.5.5",
    "polythene-css-svg": "1.5.5",
    "polythene-css-switch": "1.5.5",
    "polythene-css-tabs": "1.5.5",
    "polythene-css-textfield": "1.5.5",
    "polythene-css-toolbar": "1.5.5",
    "polythene-css-typography": "1.5.5"
  },
  "devDependencies": {
    "polythene-scripts": "1.5.5"
  },
  "author": "Arthur Clemens <arthurclemens@gmail.com> (http://arthurclemens.com)",
  "homepage": "https://github.com/ArthurClemens/polythene",
  "license": "MIT"
}
