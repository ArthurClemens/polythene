{
  "name": "polythene",
  "version": "0.1.2",
  "description": "Polymer inspired modular implementation of Material Design for Mithril.",
  "license": "MIT",
  "main": "",
  "directories": {},
  "dependencies": {
    "material-colors": "^1.0.0",
    "material-design-iconic-font": "^2.1.2"
  },
  "devDependencies": {
    "shelljs": "",
    "rimraf": "",
    "mkdirp": "",
    "cpr": "",
    "find": "",
    "sass": "",
    "mithril": "^0.2.0",
    "autoprefixer": "",
    "clean-css": "",
    "uglify-js": "",
    "babel": "",
    "babel-plugin-closure-elimination": "",
    "chokidar": "",
    "webfont-dl": ""
  },
  "scripts": {
    "postinstall": "node scripts/copy-deps.js",
    "watch-css": "node scripts/watch-scss.js lib/",
    "watch-es6": "node scripts/watch-es6.js lib/",
    "watch": "npm run watch-css & npm run watch-es6",
    "transpile-css": "node scripts/watch-scss.js lib/ null once",
    "transpile-js": "node scripts/watch-es6.js lib/ null once",
    "transpile": "npm run transpile-css && npm run transpile-js",
    "clean": "rimraf node_modules && rimraf external_modules && rimraf lib/deps/ && find ./lib -name \"*.js\" -not -name \"*.es6.js\" -print | xargs rimraf && find ./lib -name \"*.css\" -print | xargs rimraf"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ArthurClemens/Polythene.git"
  },
  "author": "Arthur Clemens",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/mit-license.php"
    }
  ],
  "bugs": {
    "url": "https://github.com/ArthurClemens/Polythene/issues"
  },
  "homepage": "https://github.com/ArthurClemens/Polythene"
}
